<?xml version="1.0" encoding="UTF-8"?>
<SAR>
    <onstartpic>onstart01.png</onstartpic>
    <onstartheader>SAR图像地面站辅助判读系统</onstartheader>
    <mainwindowheader>SAR图像地面站辅助判读系统</mainwindowheader>
    <algorithmpath>algofiles</algorithmpath>
    <filelistname>文件列表</filelistname>
    <algorithmlistname>SAR目标检测算法</algorithmlistname>
    <algorithmlist>
        <algorithm>
            <algorithm-name>拉普拉斯滤波法</algorithm-name>
            <algorithm-local>add_obj.py</algorithm-local>
            <profile>true</profile>
            <algorithm-threshold>
                <default>7</default>
                <min>0</min>
                <max>255</max>
            </algorithm-threshold>
        </algorithm>
        <algorithm>
            <algorithm-name>灰度直方图特征法</algorithm-name>
            <algorithm-local>data_augmentation_check.py</algorithm-local>
            <algorithm-threshold>
                <default>1</default>
                <min>0</min>
                <max>10</max>
            </algorithm-threshold>
        </algorithm>
        <algorithm>
            <algorithm-name>基于滑窗的能量特征目标检测方法1</algorithm-name>
            <algorithm-local></algorithm-local>
            <algorithm-threshold>
                <default>3</default>
                <min>0</min>
                <max>10</max>
            </algorithm-threshold>
            <window_size>
                <height></height>
                <width></width>
            </window_size>
        </algorithm>
        <algorithm>
            <algorithm-name>基于矩形纹理特征目标检测方法</algorithm-name>
            <algorithm-local></algorithm-local>
            <algorithm-threshold>
                <default>0</default>
                <min>0</min>
                <max>255</max>
            </algorithm-threshold>
        </algorithm>
        <algorithm>
            <algorithm-name>基于滑窗的能量特性检测方法2</algorithm-name>
            <algorithm-local></algorithm-local>
            <algorithm-threshold>
                <default>0</default>
                <min>0</min>
                <max>10</max>
            </algorithm-threshold>
            <window_size>
                <height></height>
                <width></width>
            </window_size>
        </algorithm>
        <algorithm>
            <algorithm-name>基于纹理的SAR图像目标检测方法</algorithm-name>
            <algorithm-local></algorithm-local>
            <algorithm-threshold>
                <default>0</default>
                <min>0</min>
                <max>255</max>
            </algorithm-threshold>
        </algorithm>
        <algorithm>
            <algorithm-name>基于信噪比的SAR图像目标检测方法</algorithm-name>
            <algorithm-local></algorithm-local>
            <algorithm-threshold>
                <default>0</default>
                <min>0</min>
                <max>255</max>
            </algorithm-threshold>
        </algorithm>
        <algorithm>
            <algorithm-name>基于圆形纹理特征目标检测方法</algorithm-name>
            <algorithm-local></algorithm-local>
            <algorithm-threshold>
                <default>0</default>
                <min>0</min>
                <max>10</max>
            </algorithm-threshold>
        </algorithm>
    </algorithmlist>
</SAR>
